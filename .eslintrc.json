{
  "env": {
    "browser": true,
    "es2021": true,
    "jest": true
  },
  "extends": [
    "airbnb-base"
  ],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "plugins": [
    "@typescript-eslint",
    "jest"
  ],
  "rules": {},
  "overrides": [
    {
      "files": ["*.ts", "*.js"],
      "rules": {
        "import/extensions": "off",
        "import/no-unresolved": "off",
        "import/prefer-default-export": "off",
        "no-shadow": "off",
        "no-unused-vars": "off",
        "class-methods-use-this": "off",
        "no-underscore-dangle": "off",
        "no-use-before-define": "off"
      }
    }
  ]
}
